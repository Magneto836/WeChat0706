.circleImage {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 100rpx;
  height: 245rpx;
  width: 245rpx;
  border-radius: 50%;
}

.userNickname {
  margin-top: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: large;
}

.welcome-text {
  text-align: center;
  margin-top: 20rpx; /* Adjust margin as needed */
  font-size: 32rpx; /* Adjust font size as needed */
}

.label-list {
  display: flex;
  width: 100%;
  height: 120rpx;
  align-items: center;
  border-bottom: 1rpx solid #e8e8e8;
}

.label-list .label {
  color: #333;
  font-size: 36rpx;
  margin-left: 100rpx;
  font-weight: bold;
}

.input-box {
  flex: 1; /* Take remaining space in the flex container */
  display: flex;
  align-items: center;
  padding-left: 20rpx; /* Adjust padding as needed */
}
.ShowBtn:not([size='mini']){ 
  display: flex;
   align-items: center;
  justify-content: center;
     background-color: white; 
     margin-right: 50rpx;
     width:250rpx; 
     height:70rpx;
     
      color: green;
 }
.buttonType {
  margin-left: 200rpx;
  margin-top: 20rpx;
}
page{
  height:100%
}
.page-container {
  background-color: #cbf1f5; 
  padding: 20rpx; /* 页面内容的内边距，根据需要调整 */
  width: 100%;
  height: 100%; /* 让容器高度占满整个页面 */
}

.WelcomeTextStyle {        
  font-family: "宋体";
  font-size: 35rpx; /* 根据需要调整字体大小 */
  line-height: 1.2; /* 根据需要调整行高 */
  font-weight: bold;
}
